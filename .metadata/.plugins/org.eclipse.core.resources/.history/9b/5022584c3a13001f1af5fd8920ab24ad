package com.example.tabelog_nagoyameshi.form;

import lombok.Data;

@Data
public class FavoriteRegisterForm {
	@NotNull
	private Integer userId;
	
	@NotNull
	private Integer shopId;
	
	@Transactional
	public void deleteByUserIdAndShopId(Integer userId, Integer shopId) {
		this.userId = userId;
		this.shopId = shopId;
	}

}